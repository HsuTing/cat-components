{
  "scripts": {
    "copy:multiple": "rm -rf ./templates-multiple && cp -r ./src/multiple ./templates-multiple",
    "copy:templates": "rm -rf ./templates-templates && cp -r ./src/templates ./templates-templates",
    "time-tracker": "time-tracker",
    "build-color": "node ./bin/build-color.js",
    "test": "yarn test:babel && yarn test:static && istanbul cover _mocha -- -R spec test/**/*.js",
    "test:babel": "yarn babel && BABEL_ENV=static babel docs/src/test --out-dir lib/docs/test",
    "test:static": "node ./lib/bin/static.js ./test.config.js",
    "webpack-server": "webpack-dev-server --content-base src --hot --inline",
    "webpack": "NODE_ENV=production webpack",
    "lint": "eslint --cache ./src ./bin/*.js ./docs/src --ext .js",
    "lint:watch": "esw --cache ./src ./bin/*.js ./docs/src --ext .js -w --color",
    "babel": "rm -rf ./lib && babel src --out-dir lib",
    "babel:watch": "rm -rf ./lib && babel -w src --out-dir lib",
    "prepublish": "yarn babel && yarn copy:multiple && yarn copy:templates",
    "build": "yarn build-color && yarn babel && yarn static",
    "prod": "export NODE_ENV=production && yarn build-color && yarn babel && yarn static && yarn webpack",
    "watch": "concurrently -c green \"yarn lint:watch\" \"yarn babel:watch\" \"yarn webpack-server\"",
    "static": "BABEL_ENV=static babel --ignore test docs/src --out-dir lib/docs && node ./lib/bin/static.js static.config.js"
  },
  "name": "cat-components",
  "version": "1.6.9",
  "description": "react components",
  "author": {
    "name": "hsuting",
    "email": "hsuting0106@gmail.com",
    "url": "http://hsuting.com"
  },
  "keywords": [
    "react",
    "components"
  ],
  "pre-commit": [
    "lint"
  ],
  "bin": {
    "static": "./lib/bin/static.js",
    "copy": "./lib/bin/copy.js"
  },
  "homepage": "https://github.com/HsuTing/cat-components/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HsuTing/cat-components.git"
  },
