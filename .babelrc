{
  "env": {
    "static": {
      "plugins": [
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "cat-components/lib": "lib"
          }
        }]
      ]
    }
  },
  "presets": [
    "react",
    "env",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-assign",
    ["module-resolver", {
      "root": ["./", "./src"],
      "alias": {
        "cat-components/lib": "src",
        "color": "color",
        "utils": "utils"
      }
    }]
  ]
}
